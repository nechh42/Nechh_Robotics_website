{
    "version": 2,
    "builds": [
        {
            "src": "api/*.js",
            "use": "@vercel/node"
        },
        {
            "src": "*.html",
            "use": "@vercel/static"
        },
        {
            "src": "assets/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/nechh-data",
            "dest": "/api/nechh-data.js"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
}